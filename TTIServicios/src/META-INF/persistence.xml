<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="TTIServicios"
		transaction-type="RESOURCE_LOCAL">
		<class>ttiws.model.ArchivoModel</class>
		<class>ttiws.model.CalificacionModel</class>
		<class>ttiws.model.CarreraModel</class>
		<class>ttiws.model.DepartamentoModel</class>
		<class>ttiws.model.EscuelaModel</class>
		<class>ttiws.model.PersonaModel</class>
		<class>ttiws.model.PersonaHasCarreraModel</class>
		<class>ttiws.model.PersonaHasCarreraModelPK</class>
		<class>ttiws.model.PersonaHasPersonaModel</class>
		<class>ttiws.model.PersonaHasPersonaModelPK</class>
		<class>ttiws.model.PersonaHasRolModel</class>
		<class>ttiws.model.PersonaHasRolModelPK</class>
		<class>ttiws.model.PersonaHasTtituloModel</class>
		<class>ttiws.model.PersonaHasTtituloModelPK</class>
		<class>ttiws.model.RegistroreunionModel</class>
		<class>ttiws.model.ReunionModel</class>
		<class>ttiws.model.RolModel</class>
		<class>ttiws.model.TtituloModel</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ttidesa" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="" />
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.batch-writing" value="JDBC" />
		</properties>
	</persistence-unit>
</persistence>
